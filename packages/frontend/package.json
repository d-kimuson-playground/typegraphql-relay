    "@vitejs/plugin-react": "^2.2.0",
    "babel-plugin-relay": "^14.1.0",
    "graphql": "^16.6.0",
    "npm-run-all": "^4.1.5",
    "relay-compiler": "^14.1.0",
    "typescript": "^4.9.3",
